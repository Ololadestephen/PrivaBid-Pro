#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/apple/Downloads/PrivaBid/node_modules/.pnpm/@fhevm+sdk@0.7.0-20/node_modules/@fhevm/sdk/bin/node_modules:/Users/apple/Downloads/PrivaBid/node_modules/.pnpm/@fhevm+sdk@0.7.0-20/node_modules/@fhevm/sdk/node_modules:/Users/apple/Downloads/PrivaBid/node_modules/.pnpm/@fhevm+sdk@0.7.0-20/node_modules/@fhevm/node_modules:/Users/apple/Downloads/PrivaBid/node_modules/.pnpm/@fhevm+sdk@0.7.0-20/node_modules:/Users/apple/Downloads/PrivaBid/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/apple/Downloads/PrivaBid/node_modules/.pnpm/@fhevm+sdk@0.7.0-20/node_modules/@fhevm/sdk/bin/node_modules:/Users/apple/Downloads/PrivaBid/node_modules/.pnpm/@fhevm+sdk@0.7.0-20/node_modules/@fhevm/sdk/node_modules:/Users/apple/Downloads/PrivaBid/node_modules/.pnpm/@fhevm+sdk@0.7.0-20/node_modules/@fhevm/node_modules:/Users/apple/Downloads/PrivaBid/node_modules/.pnpm/@fhevm+sdk@0.7.0-20/node_modules:/Users/apple/Downloads/PrivaBid/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/fhevm.js" "$@"
else
  exec node  "$basedir/../../bin/fhevm.js" "$@"
fi
